    "clean:program-rust": "cargo clean --manifest-path=./src/program-rust/Cargo.toml && rm -rf ./dist",
    "test:program-rust": "cargo test-bpf --manifest-path=./src/program-rust/Cargo.toml",
    "pretty": "prettier --write '{,src/**/}*.ts'"
